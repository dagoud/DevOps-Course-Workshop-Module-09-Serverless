{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "msg",
      "type": "queueTrigger",
      "direction": "in",
      "queueName": "acmesub-translations-queue",
    },
    {
      "name": "messageJSON",
      "type": "table",
      "tableName": "AcmeTranslations",
      "partitionKey": "message",
      "rowKey": "{rowKey}",
      "direction": "in"
    },
    {
      "name": "message",
      "type": "table",
      "tableName": "AcmeSubitles",
      "partitionKey": "message",
      "direction": "out"
    }
  ]
}